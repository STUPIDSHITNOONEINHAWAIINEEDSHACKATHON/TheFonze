{
  "manifest_version": 2,

  "name": "Feeling Happy?",
  "description": "This extension will fuck your shit up",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html",
   "default_title": "Click here!"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["./js/search.js"]

    }
  ],

  "permissions": [
   "activeTab"
   ]
}